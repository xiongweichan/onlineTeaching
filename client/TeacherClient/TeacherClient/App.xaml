<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TeacherClient"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="TeacherClient.App"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ButtonDictionary.xaml"/>
                <ResourceDictionary Source="TextBoxDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:TextToVisibilityConverter x:Key="TextToVisibilityConverter"/>
            <System:Double x:Key="FontSize1">14</System:Double>
        </ResourceDictionary>
    </Application.Resources>
</Application>
